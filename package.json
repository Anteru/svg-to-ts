{
  "name": "tsvg",
  "version": "1.0.0",
  "description": "Command line util that converts svgs from a directory into a single TypeScript file",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node index.ts",
    "format:check": "prettier --list-different 'src/**/*.ts'",
    "format:write": "prettier --write 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kreuzerk/tsvg.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bin": {
    "tsvg": "./src/bin/tsgv.bin.js"
  },
  "keywords": [
    "SVG",
    "TypeScript",
    "Icons",
    "Build-tool"
  ],
  "author": "Kevin Kreuzer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kreuzerk/tsvg/issues"
  },
  "homepage": "https://github.com/kreuzerk/tsvg#readme",
  "devDependencies": {
    "@types/node": "^12.12.8",
    "husky": "^3.1.0",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.19.1",
    "svgo": "^1.3.2"
  }
}
